{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:smoke": "NODE_ENV=development next dev -p 3000",
    "restart": "pkill -f \":3000\" || true; sleep 0.5; npm run dev",
    "smoke:loop": "node scripts/dev-loop.js",
    "dev:force": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && next dev --turbopack --port 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint",
    "clean-ports": "lsof -ti:3000,3001 | xargs kill -9 2>/dev/null || echo 'No processes to kill'",
    "smoke": "tsx scripts/smoke.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.32.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.2",
    "@types/cheerio": "^0.22.35",
    "@types/jsonwebtoken": "^9.0.10",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cuid": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "openai": "^5.19.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "tsx": "^4.19.0",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "wait-on": "^7.2.0"
  }
}
