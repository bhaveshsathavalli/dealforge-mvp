{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:smoke": "NODE_ENV=development next dev -p 3000",
    "restart": "pkill -f \":3000\" || true; sleep 0.5; npm run dev",
    "smoke:loop": "node scripts/dev-loop.js",
    "dev:force": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && next dev --turbopack --port 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint",
    "clean-ports": "lsof -ti:3000,3001 | xargs kill -9 2>/dev/null || echo 'No processes to kill'",
    "smoke": "tsx scripts/smoke.ts",
    "dev:classify": "tsx scripts/dev-classify-url.ts",
    "dev:crawl": "tsx scripts/dev-crawl.ts",
    "dev-pricing": "tsx scripts/dev-pricing.ts",
    "dev-features": "tsx scripts/dev-features.ts",
    "dev-integrations": "tsx scripts/dev-integrations.ts",
    "smoke:facts": "tsx scripts/dev-extract.ts",
    "smoke:facts:pair": "tsx scripts/facts-smoke.ts",
    "backfill:org": "tsx scripts/backfill-org-id.ts --apply",
    "check:env": "tsx scripts/check-env-at-boot.ts",
    "test:competitors:watch": "vitest tests/competitors.delete.test.ts --watch=false --reporter=dot || exit 1",
    "fix:competitors:loop": "node scripts/fix-competitors-loop.js",
    "test:api:team": "vitest run src/tests/api/team.manage.test.ts --reporter=dot",
    "test:e2e:team": "playwright test src/tests/e2e/team.admin.spec.ts",
    "test:api": "vitest run src/tests/api --reporter=verbose",
    "test:e2e": "playwright test src/tests/e2e/team.flow.spec.ts --reporter=line",
    "fix:team:loop": "tsx scripts/fix-team-loop.ts",
    "test:api:invite": "vitest run src/tests/api/team.invite.test.ts --reporter=dot",
    "test:e2e:invite": "playwright test src/tests/e2e/team.invite.spec.ts",
    "test:api:invites": "vitest run src/tests/api/invites.clerk.test.ts --reporter=dot",
    "test:e2e:invites": "playwright test src/tests/e2e/invites.clerk.spec.ts",
    "dev:test": "next dev -p 3000",
    "fix:invite:loop": "node scripts/fix-invite-loop.js",
    "fix:invites:loop": "node scripts/fix-invites-loop.js"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.32.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.2",
    "@types/cheerio": "^0.22.35",
    "@types/jsonwebtoken": "^9.0.10",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cuid": "^3.0.0",
    "dotenv": "^17.2.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "openai": "^5.19.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "stripe": "^18.5.0",
    "svix": "^1.76.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "playwright": "^1.55.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.0",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "wait-on": "^7.2.0"
  }
}
